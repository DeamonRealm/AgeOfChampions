  <!--Alert snippet-->
  <CodeSnippet Format="1.0.0">  
      <Header>  
        <Title>ALERT</Title>  
			  <Author>anonymous</Author>  
			  <Description>Print a LOG alert</Description>  
			  <Shortcut>log_alert</Shortcut>
      </Header>  
      <Snippet>  
          <Code Language="CPP">  
              <![CDATA[LOG("ALERT! ---------------------------------------------------- ");]]>  
          </Code>  
      </Snippet>  
  </CodeSnippet>

  <!--List iterator snippet-->
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>list_iteration</Title>
      <Author>anonymous</Author>
      <Description>Print a std::list iteration</Description>
      <Shortcut>list_iteration</Shortcut>
    </Header>
    <Snippet>
      <Code Language="CPP">
        <![CDATA[
        std::list<data>::iterator item = $my_list$.begin();
        while(item != $my_list$.end())
        {
        
          item++;
        }
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  
